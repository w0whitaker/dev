---js
const eleventyNavigation = {
	key: "Home",
	order: 1
};

const numberOfLatestPostsToShow = 3;
---
<h2>Thoughtables.</h2>
<ul>
	{% for post in collections.posts %}
		<li>
			<p>
				<a href="{{ post.url }}">
					{{ post.data.title }}
				</a>
			</p>
			<p>
				<time datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate("DDD") }}</time>
			</p>
		</li>
	{% endfor %}
</ul>